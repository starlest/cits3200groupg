package com.example.guildwayfinding;

public class Staff {
	private String name;
	private int room;
	private String faculty;
	private String schedule;
	private String telephone;
	private String email;

	
	public Staff(String name, int room, String faculty, String schedule, String telephone, String email) 
	{
		this.name = name;
		this.room = room;
		this.faculty = faculty;
		this.schedule = schedule;
		this.telephone = telephone;
		this.email = email;
	}
}
